# Configuración del servidor
PORT=3000
NODE_ENV=development
FRONTEND_URL=http://localhost:3000

# Configuración de Supabase
SUPABASE_URL=tu_url_de_supabase
SUPABASE_KEY=tu_clave_anonima_de_supabase

# Configuración de la base de datos PostgreSQL
POSTGRES_DB=travelquest
POSTGRES_USER=tu_usuario
POSTGRES_PASSWORD=tu_contraseña
POSTGRES_HOST=tu_host
POSTGRES_PORT=5432
POSTGRES_SSL=true

# Configuración de JWT para autenticación
JWT_SECRET=tu_secreto_jwt_muy_seguro
JWT_EXPIRES_IN=24h

# Configuración de OpenAI (para el chat)
OPENAI_API_KEY=tu_clave_de_openai

# Configuración de Google Vision (opcional, para validación de imágenes)
GOOGLE_VISION_API_KEY=tu_clave_de_google_vision

# Configuración de almacenamiento
UPLOAD_DIR=uploads
MAX_FILE_SIZE=5242880  # 5MB en bytes 